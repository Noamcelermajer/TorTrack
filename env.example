# Docker Compose Environment Variables
# Copy this file to .env in the root directory

# API Keys (REQUIRED - Get these from the respective services)
PROWLARR_API_KEY=your_prowlarr_api_key_here
TMDB_API_KEY=your_tmdb_api_key_here

# Service URLs (Change if using different hosts/ports)
PROWLARR_URL=http://prowlarr:9696

# qBittorrent Credentials
QBITTORRENT_USERNAME=admin
QBITTORRENT_PASSWORD=changeme

# Docker User/Group IDs (for file permissions)
PUID=1000
PGID=1000
TZ=UTC 